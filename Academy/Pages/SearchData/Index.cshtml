            flex-direction: column;
            align-items: center;
            padding: 20px;
        }

        input[type="text"] {
            width: 90%;
            max-width: 500px;
            padding: 15px;
            margin: 10px 0;
            border: 1px solid #007bff;
            border-radius: 5px;
            font-size: 16px;
            transition: border-color 0.3s;
        }

            input[type="text"]:focus {
                border-color: #0056b3;
                outline: none;
            }

        button {
            padding: 10px 20px;
            margin: 5px;
            font-size: 16px;
            color: #fff;
            background-color: #007bff;
            border: none;
            border-radius: 5px;
            cursor: pointer;
            transition: background-color 0.3s, transform 0.3s;
        }

            button:hover {
                background-color: #0056b3;
                transform: scale(1.1);
            }

            button:active {
                transform: scale(1.05);
            }

        #verifyBox {
            display: none;
            background: #fff;
            border-radius: 10px;
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);
            padding: 20px;
            margin: 20px 0;
            width: 90%;
            max-width: 500px;
            text-align: center;
        }

            #verifyBox h3 {
                margin: 0 0 15px 0;
                font-size: 20px;
                color: #007bff;
                border-bottom: 2px solid #007bff;
                display: inline-block;
                padding: 5px 10px;
            }

        #result {
            margin: 20px 0;
            padding: 15px;
            font-size: 20px;
            font-weight: bold;
            color: #fff;
            border-radius: 8px;
            text-align: center;
            display: none;
        }

        #userDetails {
            margin: 15px 0;
            padding: 20px;
            font-size: 16px;
            background-color: #28a745;
            border-radius: 8px;
            text-align: center;
            display: none;
        }

            #userDetails h4 {
                margin: 10px 0;
                color: #fff;
                font-size: 18px;
            }

        ul.suggestions {
            width: 90%;
            max-width: 500px;
            margin: 0;
            padding: 0;
            list-style: none;
            background: #fff;
            border: 1px solid #007bff;
            border-radius: 5px;
            max-height: 150px;
            overflow-y: auto;
            position: absolute;
            z-index: 100;
        }

            ul.suggestions li {
                padding: 10px;
                cursor: pointer;
                border-bottom: 1px solid #ddd;
            }

                ul.suggestions li:last-child {
                    border-bottom: none;
                }

                ul.suggestions li:hover {
                    background-color: #f0f8ff;
                }

        #loadingSpinner {
            font-size: 18px;
            color: #007bff;
            margin: 15px 0;
            text-align: center;
            display: none;
        }

