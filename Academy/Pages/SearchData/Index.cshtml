                let selectedParent = null;

              function normalizeString(str) {
            const normalizationMap = {
                "أ": "ا", "إ": "ا", "آ": "ا", "ؤ": "و", "ئ": "ي", "ى": "ي", "ة": "ه"
            };

            // إزالة المسافات وتطبيق خريطة التطبيع
            return str.replace(/\s+/g, "").split("").map(char => normalizationMap[char] || char).join("");
        }


                function showSuggestions() {
                    const parentNameInput = document.getElementById("parentName").value.trim();
                    const suggestionsList = document.getElementById("suggestions");
                    const normalizedParentNameInput = normalizeString(parentNameInput.toLowerCase());

                    const matches = data.filter(item => normalizeString(item.ParentName.toLowerCase()).includes(normalizedParentNameInput));

                    suggestionsList.innerHTML = "";
                    matches.forEach(match => {
                        const li = document.createElement("li");
                        li.textContent = match.ParentName;
                        li.onclick = () => {
                            document.getElementById("parentName").value = match.ParentName;
                            suggestionsList.innerHTML = "";
                        };
                        suggestionsList.appendChild(li);
                    });
                    suggestionsList.style.display = matches.length ? "block" : "none";
                }

                function startVerification() {
                    const parentNameInput = document.getElementById("parentName").value.trim();
                    const normalizedParentName = normalizeString(parentNameInput.toLowerCase());
                    selectedParent = data.find(parent => normalizeString(parent.ParentName.toLowerCase()) === normalizedParentName);

                    document.getElementById("loadingSpinner").style.display = "none";
                    document.getElementById("errorBox").style.display = "none";

                    if (selectedParent) {
                        document.getElementById("verifyBox").style.display = "block";
                    } else {
                        document.getElementById("errorBox").style.display = "block";
                        document.getElementById("whatsappButton").style.display = "block"; // Show WhatsApp button
                    }
                }

                function verifyChildName() {
                    const childNameInput = document.getElementById("childName").value.trim();
                    const normalizedChildName = normalizeString(childNameInput);

                    if (selectedParent && selectedParent.Trainees.some(child => normalizeString(child).includes(normalizedChildName))) {
                        document.getElementById("userName").textContent = selectedParent.UserName;
                        document.getElementById("password").textContent = "123456789";
                        document.getElementById("userDetails").style.display = "block";
                        document.getElementById("result").textContent = "التحقق ناجح!";
                        document.getElementById("result").style.backgroundColor = "#28a745";
                        document.getElementById("result").style.display = "block";
                        document.getElementById("copyUsernameButton").style.display = "block";
                    } else {
                        document.getElementById("result").textContent = "الاسم غير موجود ضمن الأبناء";
                        document.getElementById("result").style.backgroundColor = "#dc3545";
                        document.getElementById("result").style.display = "block";
                        document.getElementById("whatsappButton").style.display = "block"; // Show WhatsApp button
                        document.getElementById("whatsappContact").style.display = "block"; // Show WhatsApp contact button
                    }
                }

                function copyUsername() {
                    const username = document.getElementById("userName").textContent;
                    const tempInput = document.createElement("input");
                    document.body.appendChild(tempInput);
                    tempInput.value = username;
                    tempInput.select();
                    document.execCommand("copy");
                    document.body.removeChild(tempInput);
                    alert("تم نسخ اسم المستخدم!");
                }
